{
  "expo": {
    "name": "mend",
    "slug": "mend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/splashimage.png",
      "resizeMode": "contain",
      "backgroundColor": "#7F3DFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.khalifa3164.mend"
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "description": "",
    "githubUrl": "https://github.com/Mend-Mobile-HQ/mend-mobile"
  }
}